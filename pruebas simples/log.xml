<sequence name="logAllProperties">
    <!-- Las unicas formas que encontre para obtener estos valores -->
    <log level="custom" category='INFO'>
        <property name="HTTP_METHOD" expression="$axis2:HTTP_METHOD"/>
        <property name="HTTP_METHOD_" expression="get-property('axis2', 'HTTP_METHOD')"/>
    </log>
    <log level="custom" category='INFO'>
        <property name='CT_trp' expression='$trp:Content-Type' />
        <property name='CT_tr' expression='get-property("transport", "Content-Type")' />
    </log>
</sequence>